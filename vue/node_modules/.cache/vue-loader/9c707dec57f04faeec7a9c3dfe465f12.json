n;\n      }\n      try {\n        const credentials = JSON.stringify({\n          username: this.form.username,\n          password: this.form.password,\n        });\n        this.signup_user(credentials);\n        this.$router.push(\"/\");\n      } catch (error) {\n        this.msg = \"Please try again.\";\n      }\n    },\n  },\n};\n</script>"]}]}