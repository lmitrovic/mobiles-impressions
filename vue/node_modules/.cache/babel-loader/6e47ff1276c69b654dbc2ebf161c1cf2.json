t":["import Vue from 'vue'\nimport Vuex from 'vuex'\n\nVue.use(Vuex);\n\nconst apiBaseUrl = 'http://localhost:8080';\n\nexport default new Vuex.Store({\n  state: {\n    mobiles: [],\n    comments: []\n  },\n\n  mutations: {\n    set_mobiles: function (state, mobiles) {\n      state.mobiles = mobiles;\n    },\n\n    set_comments: function (state, comments) {\n      state.comments = comments;\n    },\n\n    add_mobile: function (state, mobile) {\n      state.mobiles.push(mobile);\n    },\n\n    remove_mobile: function (state, id) {\n      for (let m = 0; m < state.mobiles.length; m++) {\n        if (state.mobiles[m].id === id) {\n          state.mobiles.splice(m, 1);\n          break;\n        }\n      }\n    },\n\n    update_mobile: function (state, payload) {\n      for (let m = 0; m < state.mobiles.length; m++) {\n        if (state.mobiles[m].id === parseInt(payload.id)) {\n          state.mobiles[m].model = payload.mob.model;\n          state.mobiles[m].producer = payload.mob.producer;\n          break;\n        }\n      }\n    }\n  },\n\n  actions: {\n    load_mobiles: function ({ commit }) {\n      fetch(apiBaseUrl + '/api/mobiles', { method: 'get' }).then((response) => {\n        if (!response.ok)\n          throw response;\n\n        return response.json()\n      }).then((jsonData) => {\n        commit('set_mobiles', jsonData)\n      }).catch((error) => {\n        if (typeof error.text === 'function')\n          error.text().then((errorMessage) => {\n            alert(errorMessage);\n          });\n        else\n          alert(error);\n      });\n    },\n\n    load_comments: function ({ commit }, model) {\n      console.log(model.model);\n      fetch(apiBaseUrl + `/api/comments/${model}`, { method: 'get'}).then((response) => {\n        if (!response.ok)\n          throw response;\n\n        return response.json()\n      }).then((jsonData) => {\n        commit('set_comments', jsonData)\n      }).catch((error) => {\n        if (typeof error.text === 'function')\n          error.text().then((errorMessage) => {\n            alert(errorMessage);\n          });\n        else\n          alert(error);\n      });\n    },\n\n    delete_mobile: function({ commit }, id) {\n      fetch(apiBaseUrl + `/api/mobile/${id}`, { method: 'delete' }).then((response) => {\n        if (!response.ok)\n          throw response;\n\n        return response.json()\n      }).then((jsonData) => {\n        commit('remove_mobile', jsonData.id)\n      }).catch((error) => {\n        if (typeof error.text === 'function')\n          error.text().then((errorMessage) => {\n            alert(errorMessage);\n          });\n        else\n          alert(error);\n      });\n    },\n\n    new_mobile: function({ commit }, mobile) {\n      fetch(apiBaseUrl + '/api/mobiles', {\n        method: 'post',\n        headers: {\n          'Content-Type': 'application/json'\n        },\n        body: mobile\n      }).then((response) => {\n        if (!response.ok)\n          throw response;\n\n        return response.json();\n      }).then((jsonData) => {\n        commit('add_mobile', jsonData);\n      }).catch((error) => {\n        if (typeof error.text === 'function')\n          error.text().then((errorMessage) => {\n            alert(errorMessage);\n          });\n        else\n          alert(error);\n      });\n    },\n\n    change_mobile: function({ commit }, payload) {\n      fetch(apiBaseUrl + `/api/mobiles/${payload.id}`, {\n        method: 'put',\n        headers: {\n          'Content-Type': 'application/json'\n        },\n        body: payload.mob\n      }).then((response) => {\n        if (!response.ok)\n          throw response;\n\n        return response.json();\n      }).then((jsonData) => {\n        commit('update_mobile', {id: payload.id, mob:jsonData});\n      }).catch((error) => {\n        if (typeof error.text === 'function')\n          error.text().then((errorMessage) => {\n            alert(errorMessage);\n          });\n        else\n          alert(error);\n      });\n    }\n  }\n})\n"]}]}